#include <iostream>

using namespace std;
int main()
{

    for(int a = 0; a < 5; a++)
    {cout << "# ";}
    cout << endl;
    for(int b = 0; b < 5; b++)
    {cout << "#"; for(int c = 0; c < 7; c++) {cout << " ";} cout << "#" << endl;}
    for(int d = 0; d < 5; d++)
    {cout << "# ";}

    return 0;
}
